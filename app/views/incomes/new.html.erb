<%= form_for(@incomes) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :category %>
  <%= f.select :category, ['Salary','Others'] %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :prijem %>
  <%= f.number_field :prijem %>
  <%= f.submit %>
<% end %>